{
  "workflow.json": {
    "description": "Tracks current workflow state and metadata",
    "schema": {
      "state_version": "string",
      "current_workflow": "string | null",
      "status": "active | completed | failed | null",
      "created_at": "ISO 8601 datetime",
      "updated_at": "ISO 8601 datetime"
    },
    "example": {
      "state_version": "1.0.0",
      "current_workflow": "auth-impl-2025-09-04-001",
      "status": "active",
      "created_at": "2025-09-04T10:00:00Z",
      "updated_at": "2025-09-04T10:15:00Z"
    }
  },
  "session-cache.json": {
    "description": "Caches spec and context data for active workflows",
    "schema": {
      "spec_cache": {
        "[spec_id]": {
          "path": "string",
          "title": "string",
          "tasks": "array of task IDs",
          "loaded_at": "ISO 8601 datetime"
        }
      },
      "context_cache": {
        "[context_key]": "any cached data"
      },
      "metadata": {
        "timestamp": "ISO 8601 datetime",
        "expires": "ISO 8601 datetime",
        "workflow_id": "string",
        "last_accessed": "ISO 8601 datetime",
        "access_count": "number",
        "auto_extend": "boolean",
        "extension_count": "number",
        "max_extensions": "number",
        "state_version": "string"
      }
    },
    "example": {
      "spec_cache": {
        "auth": {
          "path": ".agent-os/specs/auth/auth-spec.md",
          "title": "Authentication System",
          "tasks": ["1.1", "1.2", "1.3"],
          "loaded_at": "2025-09-04T10:00:00Z"
        }
      },
      "context_cache": {
        "project_type": "node",
        "framework": "express"
      },
      "metadata": {
        "timestamp": "2025-09-04T10:00:00Z",
        "expires": "2025-09-04T10:05:00Z",
        "workflow_id": "auth-impl-2025-09-04-001",
        "last_accessed": "2025-09-04T10:02:00Z",
        "access_count": 3,
        "auto_extend": true,
        "extension_count": 1,
        "max_extensions": 12,
        "state_version": "1.0.0"
      }
    }
  },
  "command-state.json": {
    "description": "Shares data between commands in a workflow",
    "schema": {
      "[command_name]": {
        "last_run": "ISO 8601 datetime",
        "data": "any command-specific data"
      }
    },
    "example": {
      "create-spec": {
        "last_run": "2025-09-04T10:00:00Z",
        "data": {
          "spec_id": "auth",
          "path": ".agent-os/specs/auth/auth-spec.md"
        }
      },
      "create-tasks": {
        "last_run": "2025-09-04T10:05:00Z",
        "data": {
          "tasks_created": 15,
          "path": ".agent-os/tasks/auth/"
        }
      }
    }
  }
}